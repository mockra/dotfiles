# This tmux statusbar config was created by tmuxline.vim
# on Fri, 01 Mar 2019

set -g status-justify "left"
set -g status "on"
set -g status-attr "none"
set -g message-command-bg "#5f6a8e"
set -g status-left-length "100"
set -g pane-active-border-fg "#bd93f9"
set -g status-bg "#44475a"
set -g message-command-fg "#f8f8f2"
set -g pane-border-fg "#5f6a8e"
set -g message-bg "#5f6a8e"
set -g status-left-attr "none"
set -g status-right-attr "none"
set -g status-right-length "100"
set -g message-fg "#f8f8f2"
setw -g window-status-fg "#f8f8f2"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "#44475a"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "#bd93f9"
setw -g window-status-separator ""
setw -g window-status-bg "#44475a"
set -g status-left ""
set -g status-right "#[fg=#282a36,bg=#bd93f9] %H:%M "
setw -g window-status-format "#[fg=#f8f8f2,bg=#44475a] #I #[fg=#f8f8f2,bg=#44475a]#W "
setw -g window-status-current-format "#[fg=#44475a,bg=#5f6a8e,nobold,nounderscore,noitalics]#[fg=#f8f8f2,bg=#5f6a8e] #W #[fg=#5f6a8e,bg=#44475a,nobold,nounderscore,noitalics]"
